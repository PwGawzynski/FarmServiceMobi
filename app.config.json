{
  "expo": {
    "name": "FarmServiceMobi",
    "slug": "FarmServiceMobi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pwgawzynski.farmservice",
      "bitcode": false,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pwgawzynski.farmservice"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "16.4"
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow FarmService to use your location."
        }
      ],

      [
        "expo-camera",
        {
          "cameraPermission": "Allow FarmService to access your camera"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "b48a6345-3ec7-4a22-810a-1f6908314c3a"
      }
    }
  }
}
